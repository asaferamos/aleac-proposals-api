        function inIframe () {
          try {
              return window.self !== window.top;
          } catch (e) {
              return true;
          }
        }
        $(document).ready(function(){
          let iframe_set_backend = false
          if (iframe_set_backend && !inIframe() ) {
            location.href = location.origin + '?iframe=0'
